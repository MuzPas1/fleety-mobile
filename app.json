{
  "expo": {
    "name": "Fleety",
    "slug": "fleety",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fleety",
    "userInterfaceStyle": "light",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zoggy.fleety",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1FA4FF"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },

    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#032240"
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "1ac8339c-f11f-428c-88a1-8b300f6ae188"
      }
    },

    "owner": "zoggy"
  }
}
